<+>{% extends 'base.html.twig' %}\r\n\r\n{% block title %}Connexion !{% endblock %}\r\n\r\n{% block body %}\r\n<form method=\"post\">\r\n    {% if error %}\r\n        <div class=\"alert alert-danger\">{{ error.messageKey|trans(error.messageData, 'security') }}</div>\r\n    {% endif %}\r\n\r\n    {% if app.user %}\r\n        <div class=\"mb-3\">\r\n            You are logged in as {{ app.user.username }}, <a href=\"{{ path('app_logout') }}\">Logout</a>\r\n        </div>\r\n    {% endif %}\r\n\r\n<div class=\"jumbotron container bg-danger\">\r\n    <h1 class=\"h3 mb-3 font-weight-normal text-white\">Connectez-vous !</h1>\r\n    <div class=\"col\">\r\n        <label for=\"inputEmail\" class=\"form-label text-white\">Adresse Email</label>\r\n        <input type=\"email\" value=\"{{ last_username }}\" name=\"email\" id=\"inputEmail\"  class=\"form-control\" id=\"exampleFormControlInput1\" placeholder=\"nom@example.com\" required autofocus>\r\n    </div>\r\n    </br>\r\n    <div class=\"col\">\r\n        <label for=\"inputPassword\" class=\"form-label  text-white\">Mot de passe </label>\r\n        <input type=\"password\" name=\"password\" class=\"form-control\" id=\"inputPassword\" required>\r\n    </div>\r\n    </br>\r\n    <div class=\"col\">\r\n    <button type=\"button\" class=\"btn btn-light\">Connexion</button>\r\n    </div>\r\n</div>\r\n\r\n    <input type=\"hidden\" name=\"_csrf_token\"\r\n           value=\"{{ csrf_token('authenticate') }}\"\r\n    >\r\n\r\n    {#\r\n        Uncomment this section and add a remember_me option below your firewall to activate remember me functionality.\r\n        See https://symfony.com/doc/current/security/remember_me.html\r\n\r\n        <div class=\"checkbox mb-3\">\r\n            <label>\r\n                <input type=\"checkbox\" name=\"_remember_me\"> Remember me\r\n            </label>\r\n        </div>\r\n    #}\r\n\r\n</form>\r\n{% endblock %}\r\n
